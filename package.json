{
  "name": "VideoFrame",
  "version": "0.3.0",
  "description": "HTML5 Video - SMPTE Time Code capturing and Frame Seeking API - Version: 0.3",
  "main": "src/VideoFrame.js",
  "scripts": {
    "clean": "rm -rf coverage && rm -rf dist",
    "compile": "cross-env NODE_ENV=production babel src -d dist -s",
    "coverage": "cross-env NODE_ENV=test nyc --extension .es6 --reporter=lcov ./node_modules/.bin/mocha --compilers js:babel-core/register --require babel-polyfill && nyc --extension .es6 report",
    "coveralls": "cross-env NODE_ENV=test nyc --extension .es6 ./node_modules/.bin/mocha --compilers js:babel-core/register --require babel-polyfill && nyc --extension .es6 report --reporter=text-lcov | coveralls",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --require babel-polyfill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesleyE/VideoFrame.git"
  },
  "bugs": {
    "url": "https://github.com/WesleyE/VideoFrame/issues"
  },
  "homepage": "https://github.com/WesleyE/VideoFrame#readme",
  "keywords": [
    "SMPTE",
    "video",
    "frames",
    "timecode",
    "movie"
  ],
  "author": "Wesley Elfring <hi@wesleyelfring.nl>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2016": "^6.11.3",
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "cross-env": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "dist/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
